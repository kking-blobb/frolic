#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/Isaac/frolic-1/node_modules/.pnpm/supports-color@0.2.0/node_modules/supports-color/node_modules:/mnt/c/Users/Isaac/frolic-1/node_modules/.pnpm/supports-color@0.2.0/node_modules:/mnt/c/Users/Isaac/frolic-1/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/Isaac/frolic-1/node_modules/.pnpm/supports-color@0.2.0/node_modules/supports-color/node_modules:/mnt/c/Users/Isaac/frolic-1/node_modules/.pnpm/supports-color@0.2.0/node_modules:/mnt/c/Users/Isaac/frolic-1/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../supports-color@0.2.0/node_modules/supports-color/cli.js" "$@"
else
  exec node  "$basedir/../../../../../supports-color@0.2.0/node_modules/supports-color/cli.js" "$@"
fi
